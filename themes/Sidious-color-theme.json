  "tokenColors": [
    {
      "name": "Meta Function Definition",
      "scope": [
        "meta.body.function.definition.cpp",
        " meta.function.definition.cpp",
        "source.cpp"
      ],
      "settings": {
        "foreground": "#e1ffe7"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#686868"
      }
    },
    {
      "name": "Type",
